pipeline {
    agent none
    stages {
        stage('Build') {
            agent { label 'master' }
            steps {
                sh 'sleep 5'
            }
            steps {
                sh '''
                    sleep 5
                '''  
            }
        }

        stage('DEPLOY') {
            agent { label 'slave' } 
            steps {
                sh 'sleep 5'
            }
            steps {
                sh 'sleep 5'
            }
        }
    }
}
